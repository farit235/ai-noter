{
  "manifest_version": 3,
  "name": "Counter extensions for the world!",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/hello_extensions.png"
  },
  "background": {
    "popup": "popup/popup.js"
  },
  "permissions": [
    "storage"
  ]
}